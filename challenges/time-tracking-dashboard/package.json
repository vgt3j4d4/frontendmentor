{
  "name": "time-tracking-dashboard",
  "scripts": {
    "live-server": "live-server --port=5500 --no-browser",
    "tailwindcss": "tailwindcss -i ./css/main.css -o ./css/styles.css --watch",
    "dev": "concurrently --kill-others \"npm run tailwindcss\" \"npm run live-server\"",
    "dist": "npx copyfiles index.html css/styles.css images/* fonts/* js/* data.json dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "copyfiles": "^2.4.1",
    "live-server": "^1.2.2",
    "postcss-import": "^16.1.0",
    "tailwindcss": "^3.4.14"
  }
}
